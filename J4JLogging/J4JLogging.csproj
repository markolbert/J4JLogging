<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netstandard2.1</TargetFramework>
		<Nullable>enable</Nullable>
		<AssemblyName>J4JSoftware.Logging</AssemblyName>
		<RootNamespace>J4JSoftware.Logging</RootNamespace>
		<Authors>Mark A. Olbert</Authors>
		<Company>Jump for Joy Software</Company>
		<Product>J4JSoftware Logging Extensions for Serilog</Product>
		<Description>adds source code annotations to a Serilog logger</Description>
		<Copyright>© Mark A. Olbert all rights reserved</Copyright>
		<PackageDescription>Wrapper for Serilog ILogger to provide source code information and enable SMS logging</PackageDescription>
		<RepositoryUrl>https://github.com/markolbert/J4JLogger</RepositoryUrl>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<GeneratePackageOnBuild>false</GeneratePackageOnBuild>
		<PackageIcon>Diego nuspec.png</PackageIcon>
		<RepositoryType>git</RepositoryType>
		<Version>2.5.1.0</Version>
		<AssemblyVersion>2.5.1.0</AssemblyVersion>
		<PackageReleaseNotes>updated dependencies</PackageReleaseNotes>
	</PropertyGroup>

	<ItemGroup>
		<Compile Remove="docs\**" />
		<EmbeddedResource Remove="docs\**" />
		<None Remove="docs\**" />
		<Compile Remove="channels\base\ChannelAttribute.cs" />
		<Compile Remove="channels\base\ILogChannel.cs" />
		<Compile Remove="channels\base\IPostProcess.cs" />
		<Compile Remove="channels\base\LogChannel.cs" />
		<Compile Remove="channels\base\TextChannel.cs" />
		<Compile Remove="channels\console\ConsoleChannel.cs" />
		<Compile Remove="channels\debug\DebugChannel.cs" />
		<Compile Remove="channels\file\FileChannel.cs" />
		<Compile Remove="channels\file\FileChannel.Static.cs" />
		<Compile Remove="channels\file\IFileConfig.cs" />
		<Compile Remove="channels\ILogChannels.cs" />
		<Compile Remove="channels\LogChannels.cs" />
		<Compile Remove="channels\twilio\ITwilioConfig.cs" />
		<Compile Remove="channels\twilio\TwilioChannel.cs" />
		<Compile Remove="configuration\J4JLoggerConfigurationBuilder.cs" />
		<Compile Remove="configuration\J4JLoggerConfigurationJsonBuilder.cs" />
		<Compile Remove="configuration\J4JLoggerConfigurationNG.cs" />
		<Compile Remove="configuration\J4JLoggerConfigurationRootBuilder.cs" />
		<Compile Remove="converters\LogChannelListConverter.cs" />
		<None Include="E:\Pix\JumpForJoy\Diego nuspec.png">
		  <Pack>True</Pack>
		  <PackagePath></PackagePath>
		</None>
	</ItemGroup>

	<ItemGroup>
		<None Remove="J4JSoftware.Logging.xml" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.Extensions.Configuration" Version="5.0.0" />
		<PackageReference Include="Microsoft.Extensions.Options" Version="5.0.0" />
		<PackageReference Include="Microsoft.Extensions.Options.ConfigurationExtensions" Version="5.0.0" />
		<PackageReference Include="Serilog.Expressions" Version="1.0.0" />
		<PackageReference Include="System.Text.Json" Version="5.0.0" />
		<PackageReference Include="Twilio" Version="5.50.0" />
		<PackageReference Include="Serilog" Version="2.10.0" />
		<PackageReference Include="Serilog.Sinks.Console" Version="3.1.1">
		  <Aliases>SerilogConsole</Aliases>
		</PackageReference>
		<PackageReference Include="Serilog.Sinks.Debug" Version="1.0.1">
		  <Aliases>SerilogDebug</Aliases>
		</PackageReference>
		<PackageReference Include="Serilog.Sinks.File" Version="4.1.0" />
		<PackageReference Include="Serilog.Sinks.TextWriter" Version="2.1.0" />
	</ItemGroup>

</Project>
